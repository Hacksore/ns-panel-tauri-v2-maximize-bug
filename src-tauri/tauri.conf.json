{
  "productName": "ns-panel-tauri-v2-maximize-bug",
  "version": "0.1.0",
  "identifier": "com.ns-panel-tauri-v2-maximize-bug.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "minimizable": false,
        "maximizable": false,
        "transparent": true,
        "decorations": false,
        "shadow": false,
        "label": "main",
        "title": "test",
        "width": 420,
        "height": 600,
        "minHeight": 106,
        "minWidth": 64
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
